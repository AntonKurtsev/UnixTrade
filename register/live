<script>
    function sendAjaxForm(result_form, ajax_form, url) {

        $.ajax({
            url: url,
            type: "POST",
            dataType: 'html',
            data: $("#"+ajax_form).serialize(),
            async: true,
            success: function(responce) {
                if (JSON.parse(responce) == "New subscribe") {
                    document.getElementById("subscribe-email").reset();
                    $('#popup_sub_success').click();
                }
            },
            error: function(response) {
                document.getElementById("subscribe-email").reset();
                console.log(response);
            }
        });
    }

    $(document).ready(function() {

        $("#subscribe-btn").click(
            function() {
                sendAjaxForm('result_form_live', 'subscribe-email', '/subscribeAjax');         
                return false;
            }
        );
    });

</script>


