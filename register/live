<script>
    $(document).ready(function() {
        function sendAjaxForm(result_form, ajax_form, url) {
            const popupTittle = document.querySelector('.popup-live-success .popup-tittle');
            const popupText = document.querySelector('.popup-live-success .popup-text');
            
            $.ajax({
                url: url,
                type: "POST",
                dataType: 'html',
                data: $("#"+ajax_form).serialize(),
                async: true,
                success: function(responce) {
                    document.getElementById("ajax_form_live").reset();
                    
                    if (JSON.parse(responce) == "newUser") {
                        console.log('newUser');
                        popupTittle.innerHTML = 'Live обліковий запис створенно';
                        popupText.innerHTML = 'Дякуємо за реєстрацію! Ми відправили Вам інструкцію на email.';
                        $('#popup_live_success').click();

                    } else if (JSON.parse(responce) == "oldUser") {
                        console.log('oldUser');
                        popupTittle.innerHTML = 'Акаунт з таким email вже зареэстровано';
                        popupText.innerHTML = '';
                        $('#popup_live_success').click();

                    } else if (JSON.parse(responce) == "modifiedUser") {
                        console.log('modifiedUser');
                        popupTittle.innerHTML = 'Modified';                    
                        $('#popup_live_success').click();

                    } else {
                        console.log('Error');
                        $('#live_account_send').attr('disabled', false);
                    }
                },
                error: function(response) {
                    console.log(response);
                }
            });
        }

        const firstName = document.querySelector('#live_account_firstName');
        const lastName = document.querySelector('#live_account_lastName');
        const email = document.querySelector('#live_account_email');
        const phone = document.querySelector('#live_account_phone');

        const fieldFirstName = document.querySelector('.livereg-firstname');
        const fieldLastName = document.querySelector('.livereg-lastname');
        const fieldEmail = document.querySelector('.livereg-email');
        const fieldPhone = document.querySelector('.livereg-phone');

        $(".btn_live").click(
            function() {
                let errorCount = 0;

                if (firstName.value.length == 0) {
                    fieldFirstName.classList.add('has_error');
                    errorCount++;
                }

                if (lastName.value.length == 0) {
                    fieldLastName.classList.add('has_error');
                    errorCount++;
                }

                if (email.value.length == 0) {
                    fieldEmail.classList.add('has_error');
                    errorCount++;
                }

                if (phone.value.length < 9) {
                    errorCount++;
                    fieldPhone.classList.add('has_error');                    
                }

                if (errorCount == 0) {
                    sendAjaxForm('result_form_live', 'ajax_form_live', '/register/liveAjax');
                }
                
                return false;
            }
        );
    });
</script>

    <!-- END PAGE CONTENT  -->

